rmat('YYYY-MM-DD')))// agrega a forecastData todos los valores de cada dia en su posicion correspondiente\n    }\n  };\n  forecastData.shift(); // remuevo el primer dia\n  \n  return (\n\n    <div className=\"page-container\">\n      <Nav name={setCityName}/>\n      <div className=\"container\">\n        <div className=\"row\">\n                <div className=\"col\">\n                    <div className=\" d-flex flex-wrap justify-content-center\">\n                   {\n                      !loadingStatus &&\n                      (<>\n                      <WeatherCardToday location={CityName}/> \n                      {forecast.map((day, i) => <WeatherCard data={forecast[i]} temp={forecastData[i]} key={i}/>)}\n                      </>)\n                    }\n                    </div>\n                </div>\n        </div>\n      </div>\n  </div>\n  )\n};\n","/Users/Daniela/Desktop/WeatherApp/weather-app/src/WeatherCardToday.js",["40"],"/Users/Daniela/Desktop/WeatherApp/weather-app/src/utils.js",[],"/Users/Daniela/Desktop/WeatherApp/weather-app/src/Nav.js",["41","42","43","44","45","46"],"/Users/Daniela/Desktop/WeatherApp/weather-app/src/getIcons.js",[],"/Users/Daniela/Desktop/WeatherApp/weather-app/src/NavMobile.js",["47","48","49","50","51","52"],"import React, { useState } from 'react';\n\nexport const NavMobile = (props) => {\n\n    const [sidebar, setSidebar] = useState(false);\n    const showSidebar = () => setSidebar(!sidebar);\n    const handleClick = (cityName) => {\n        const {city}= props;\n        city(cityName);\n    };\n\n    return (\n        <>\n            <div className='navbar nav-mobile d-sm-block d-md-none bg-dark' onClick={showSidebar}>\n                <div  className='menu-bars'>\n                    <i className=\"fas fa-bars d-md-none\" onClick={showSidebar}/>\n                </div>\n            </div>\n\n            <nav className={sidebar ? 'nav-menu active bg-dark d-sm-block d-md-none' : 'nav-menu  bg-dark d-sm-block d-md-non